{
  "expo": {
    "name": "Green Care",
    "slug": "ReactNative",
    "scheme": "react",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/appLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#cccf9e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.musashi.smart-plant"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appLogo.png",
        "backgroundColor": "#cccf9e"
      },
      "package": "com.musashi.smart_plant"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-localization"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "13016734-042e-4510-8093-55bcda5eacaa"
      }
    }
  }
}